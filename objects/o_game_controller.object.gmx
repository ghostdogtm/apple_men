  for (i = 0; i &lt; map_object_count; i++) {
    with map_object_list[i] {
      instance_destroy();
    }
  }

  level_file = file_bin_open(level_file_name, 2);
  
  max_size_w = file_bin_read_byte(level_file);
  max_size_h = file_bin_read_byte(level_file);
  
  time_minutes = file_bin_read_byte(level_file);
  time_seconds = file_bin_read_byte(level_file);
  
  for (i = 0; i &lt; max_size_w; i++) {
    for (j = 0; j &lt; max_size_h; j++) {
      object_id = file_bin_read_byte(level_file);
      if (object_id != 0) {
        instance_create(map_start_x + ((i * block_width) + map_object_sprite_xoffset_list[object_id - 1]), map_start_y + (j * block_height), map_object_list[object_id - 1]);
